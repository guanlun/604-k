class ParentClass {
  void ParentClass() {}
  int m1() { return 1; }
  int m2() { return m1(); }
}

class ChildClass extends ParentClass {
  void ChildClass() {}
  int m1() { return 2; }
}

class Main {
  void Main() {
    ParentClass o1 = new ParentClass();
    ChildClass o2 = new ChildClass();
    print(o1.m1(), " ", o1.m2(), " ", o2.m1(), " ", o2.m2(), "\n");
  }
}